client.on('message', async msg => {
    if (msg.body.startsWith('!descargar ')) {
        const url = msg.body.split(' ')[1];
        if (url.includes('mediafire') || url.includes('mega')) {
            // LÃ³gica para descargar archivos
            msg.reply('Descargando archivo...');
        } else {
            msg.reply('Solo se permiten enlaces de Mediafire y Mega.');
        }
    }
});
 
